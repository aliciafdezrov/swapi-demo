{
  "name": "swapi-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "webpack serve --config ./config/webpack/dev.webpack.js",
    "start:prod": "webpack serve --config ./config/webpack/pro.webpack.js",
    "build:dev": "rimraf dist && npm run type-check && webpack --config ./config/webpack/dev.webpack.js",
    "build:prod": "rimraf dist && npm run type-check && webpack --config ./config/webpack/pro.webpack.js",
    "build:perf": "rimraf dist && npm run type-check && webpack --config ./config/webpack/perf.webpack.js",
    "test": "rimraf coverage && jest -c ./config/test/jest.js --verbose --coverage",
    "test:watch": "npm run test -- --watchAll -i",
    "test:e2e": "npm run start -- start:e2e",
    "start:e2e": "cypress open",
    "test:e2e:ci": "npm run start -- -r start:e2e:ci",
    "start:e2e:ci": "cypress run"
  },
  "author": "",
  "license": "ISC",
